    "#predicting for validation dataset\n",
    "pred_item_sim= item_sim_model.predict(validation_data)\n",
    "# calculating the RMSE\n",
    "rmse_item_sim= sum((pred_item_sim-validation_data[\"rating\"])**2)/len(validation_data)\n",
