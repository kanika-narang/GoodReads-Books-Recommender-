   "source": [
    "# Predicting the rating of the books for the users in the validation dataset\n",
    "pred=model.predict(validation_data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1.1326527768869492"
      ]
     },
     "execution_count": 21,
     "metadata": {},
     "output_type": "execute_result"
